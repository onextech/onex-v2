{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": [],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "watch": {},
    "analyze": {
      "cache": false
    }
  }
}
